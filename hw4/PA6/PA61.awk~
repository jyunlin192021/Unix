BEGIN { DE="Database Entry: " }
/^End of/ { next }
/^Ship's Database - Dilithium$/ { $0 = DE "Dilithium" }
/^Deep Space Hibernation:|^Weapons System:/ {
  split($0, A, ": ")
  DB[DE A[1]] = A[2]"\n"
  getline
}
{ gsub( "Log, Day 113", "Log - Mission Day 113") }
{ gsub( ", Supplemental", " - Mission Day 113, Supplemental") }
/^Database|^Captain's Log/ {
  K = $0
  DB[K] = ""
  getline
}
/^User/ { K = "" }
K != "" { DB[K] = DB[K] "\n"$0 }
END{
  for (key in DB){
    print key ":"
    print DB[key]
    print "----------------------"
  }
}
